generate 2 mutants
in "data/out/"
from "data/model/"
metamodel "/SecurityPoliciesExample/data/model/SecurityPolicy.ecore"

with commands {
		p1 = select one Parameter
		p2 = select one Parameter where {type = p1->type and self <> p1}
		r = select one Rule where {parameters = p1}
		modify r with {parameters -= p1, parameters += p2}
		create PolicyType with {name="class-control-ppr"}[1..5]
}