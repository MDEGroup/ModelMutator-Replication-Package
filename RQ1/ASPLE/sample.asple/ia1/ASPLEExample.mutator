generate 2 mutants
in "data/out/"
from "data/model/"
metamodel "/ASPLEExample/data/model/Asple.ecore"

with commands {

		op = select one BinaryOperator where {operator in ['add', 'subtract', 'multiply', 'divide', 'module']}
		exp = create BinaryOperator in op->right with {left = op->right, operator = 'add'}
		create Integer in exp->right with {value = 1123581321}[2..2]

}