generate 1 mutants
in "data/out/"
from "data/model/"
metamodel "/Table/data/model/Table.ecore"

with commands {
	select_table = select one Table where{rows<>null}
	create Table with{
		rows = one Row
		}[5..5]
	create Cell with{
		content= random-string(5,20)
	}[50..50]
	
	create Row with{
		cells = one Cell
	}[50..50]
	
	remove all Row where {cells = null}
	remove all Table where {rows = null}
	
}