generate 1 mutants
in "data/out/"
from "data/model/"
metamodel "/mySQL/data/model/MySQL.ecore"

with commands {

		db = select one DataBase

		ts = create Table with{
			name=random-string(5,20),
			database = db
		}[20..20]

		

		create Column with{
			name=random-string(5,20),
			type = "varchar",
			comment = "",
			table = one Table
		}[200..200]
		


		remove all Table where{columns=null}

	
}